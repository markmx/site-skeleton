{% trans_default_domain 'FOSUserBundle' %}

<form class="form-horizontal" action="{{ path('fos_user_registration_register') }}" {{ form_enctype(form) }} method="POST" class="fos_user_registration_register">
    <legend>New user account details</legend>
    {{ form_widget(form, {'form_type': 'horizontal'}) }}
    <div class="control-group">
        <div class="controls">
            <button type="submit" class="btn">{{ 'registration.submit'|trans }}</button>
        </div>
    </div>
</form>
